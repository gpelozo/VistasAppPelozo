{
  "expo": {
    "name": "VistasAppPelozo",
    "slug": "VistasAppPelozo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "src/assets/images/favicon.jpeg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "src/assets/images/favicon.jpeg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
        },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "src/assets/images/adaptive-icon.jpeg",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.jpeg"
    }
  }
}
